<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="furniture_branding_type_form_view" model="ir.ui.view">
            <field name="name">furniture.branding.type.form</field>
            <field name="model">furniture.branding.type</field>
            <field name="arch" type="xml">
                <form string="ModelTitle">
                    <sheet>

                        <group>
                            <group>
                                                                <field name="name" placeholder="Name"/>

                            </group>
                            <group>
                                                                <field name="sequence"/>
                                <field name="is_others"></field>

                            </group>
                        </group>

                    </sheet>
                </form>
            </field>
        </record>

        <record id="furniture__branding_type_tree_view" model="ir.ui.view">
            <field name="name">branding_type.tree</field>
            <field name="model">furniture.branding.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="sequence"/>
                </tree>
            </field>
        </record>

        <record id="furniture_branding_type_act_window" model="ir.actions.act_window">
            <field name="name">Furniture Branding Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">furniture.branding.type</field>
<!--            <field name="view_type">form</field>-->
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new ModelTitle.
                </p>
            </field>
        </record>

        <menuitem name="Furniture Branding " id="furniture_branding_type_menu"   parent="event.menu_event_configuration" action="furniture_branding_type_act_window"/>

    </data>
</odoo>
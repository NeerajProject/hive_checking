<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <report
    id="branding_panel_xlsx_report_xlsx"
    model="report.branding.panel"
    string="Branding Panel XLSX"
    report_type="xlsx"
    name="brand_pannel_report.branding_panel_xlsx_report_xlsx"
    file="brand_pannel_report.branding_panel_xlsx_report_xlsx"
    attachment_use="False"
/>








   <record id="branding_panel_xlsx_report_view" model="ir.ui.view">
       <field name="name">Branding Panel Report</field>
       <field name="model">report.branding.panel</field>
       <field name="arch" type="xml">
           <form string="Branding Panel Report">
               <separator string="Select Event"/>
               <group col="4">
                   <field name="event_id"/>

               </group>
               <footer>
                   <button string="Export" type="object" name="action_export_branding_panel_report"/>
                   <button string="Cancel" class="btn btn-default" special="cancel"/>
               </footer>
           </form>
       </field>
   </record>


     <record id="action_branding_panel_xlsx_repoort" model="ir.actions.act_window">
       <field name="name">Branding Panel Report</field>
       <field name="res_model">report.branding.panel</field>
       <field name="type">ir.actions.act_window</field>
       <field name="view_mode">form</field>
       <field name="target">new</field>
   </record>
   <menuitem id="menu_branding_panel_xlsx_repoort" parent="event.menu_reporting_events" name="Branding Panel Xlsx"
            action="action_branding_panel_xlsx_repoort" sequence="20" />


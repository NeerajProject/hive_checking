        <record id="email_template_exhibitor_hotel_request" model="mail.template">
            <field name="name">Exhibitor Registration: HOTEL VOUCHER</field>
            <field name="model_id" ref="techfuge_exhibitor_customisation.model_exhibitor_hotel_request"/>
